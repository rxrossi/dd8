{
  "name": "dd-manager",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start:electron": "electron .",
    "start:react": "webpack-dev-server --mode development --hot --quiet --content-base ./build",
    "build:react": "webpack --mode production",
    "build:electron:win32": "electron-builder --win --ia32",
    "dev": "concurrently \"yarn start:react\" \"wait-on http://localhost:8080 && electron .\"",
    "postinstall": "electron-builder install-app-deps",
    "build": "yarn build:react && yarn build:electron",
    "test": "jest",
    "test:dev": "jest --watch --verbose false",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm"
  },
  "build": {
    "appId": "com.dd.dd-manager",
    "productName": "Dany Dubinski"
  },
  "engines": {
    "node": "10.15.0",
    "yarn": "1.12.3"
  },
  "dependencies": {
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "appdata-path": "^1.0.0",
    "electron-is-dev": "^1.0.1",
    "final-form": "^4.11.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-final-form": "^4.0.2",
    "react-icons": "^3.2.2",
    "reakit": "^0.15.13",
    "reakit-theme-default": "^0.1.12",
    "reflect-metadata": "^0.1.10",
    "sqlite3": "^4.0.4",
    "typeorm": "0.2.9"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.15",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/jest": "^23.3.10",
    "@types/node": "^8.0.29",
    "concurrently": "^4.1.0",
    "electron": "3.0.13",
    "electron-builder": "^20.38.4",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "jest-enzyme": "^7.0.1",
    "react-test-renderer": "^16.7.0",
    "ts-jest": "^23.10.5",
    "ts-loader": "^5.3.2",
    "ts-node": "3.3.0",
    "tsconfig-paths": "^3.7.0",
    "typescript": "^3.2.2",
    "wait-on": "^3.2.0",
    "webpack": "^4.28.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.14"
  }
}
